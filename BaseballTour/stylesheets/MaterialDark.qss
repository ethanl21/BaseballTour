QWidget {
    font: "Roboto";
}
QMainWindow {
        background-color:#303030;
}
QDialog {
        background-color:#303030;
}
QColorDialog {
        background-color:#303030;
}
QTextEdit {
        background-color:#303030;
        color: #ffffff;
}
QPlainTextEdit {
	selection-background-color:#007b50;
        background-color:#424242;
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
        color: #ffffff;
}
QPushButton{
        /*border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: transparent;
	border-width: 1px;
        border-style: solid;*/
        border-radius: 2px;
        color: #ffffff;
        text-transform: uppercase;
        font-size: 10px;
        font-weight: bold;
        padding: 4px;
        background-color: #3F51B5;
}
QPushButton::default{
        border-radius: 2px;
        color: #1d1d1d;
        text-transform: uppercase;
        font-size: 10px;
        font-weight: bold;
        padding: 2px;
        background-color: #e0e0e0;
}
QToolButton {
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
        border-bottom-color: #3F51B5;
	border-bottom-width: 1px;
	border-style: solid;
        color: #ffffff;
	padding: 2px;
        background-color: #424242;
}
QToolButton:hover{
	border-style: solid;
	border-top-color: transparent;
	border-right-color: transparent;
	border-left-color: transparent;
	border-bottom-color: #37efba;
	border-bottom-width: 2px;
	border-style: solid;
	color: #FFFFFF;
	padding-bottom: 1px;
        background-color: #424242;
}
